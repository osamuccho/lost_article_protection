<div class="contents row">
  <% @events.each do |event| %>
  <div class="content_post" style="background-image: url(<%= event.event_name %>);">
     <div class="more">
        <span><%= image_tag 'arrow_top.png' %></span>
        <ul class="more_list">
          <li>
            <%= link_to '削除', "/events/#{event.id}", method: :delete %>
          </li>
        </ul>
      <span class="name">
        <%= event.event_name %>
      </span>
      </div>
  <% end %>
 </div>
</div >